<?xml version="1.0" encoding="UTF-8"?>
<module>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.common.collect.Collect'/>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='javax.persistence'/>
  <inherits name='com.bfr.client.Selection'/>

 <!-- Include GWTUpload library -->
  <inherits name="gwtupload.GWTUpload"/>
  <!-- Load dynamically predefined styles in the library when the application starts -->
  <stylesheet src="Upload.css"/>
  
  <!-- Specify the paths for translatable code                    -->
  <source path='lib/gwt'>
  	<exclude name='server/**'/>
  </source>
  <source path='lib/sexpr'>
  	<exclude name='**Test**'/>
  </source>
  <source path='lib/gwt/client'>
  	<exclude name='**Test**'/>
  </source>
  <source path='lib/util'>
  	<exclude name='TestSMap**'/>
  	<exclude name='DBScript**'/>
  	<exclude name='FileUtil.*'/>
  	<exclude name='ZipUtil.*'/>
  	<exclude name='LineWriter.*'/>
  	<exclude name='**Test**'/>
  </source>
  <source path='lib/command'>
  	<exclude name='CommandImpl**' />
  	<exclude name='CommandServer**' />
  	<exclude name='CommandService**' />
  	<exclude name='CommandWrapper**' />
  	<exclude name='RegisteredCommands**' />
  	<exclude name='jpa/**' />
  </source>
</module>
