<module rename-to='CatalogXS'>
    <!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.json.JSON' />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name='org.cobogw.gwt.user.User' />
	<inherits name='eu.future.earth.gwt.FtrGwtDate' />
	<inherits name='eu.future.earth.gwt.FtrGwtFile' />
	

	<inherits name='com.google.gwt.user.theme.standard.Standard' />

	<inherits name='eu.easyenterprise.gwt.framework.EeGwtFramework' />
 
    <!-- Specify the app entry point class.                   -->
    <entry-point class='agilexs.catalogxs.presentation.client.CatalogManager'/>

    <stylesheet src='Catalog.css'/>

    <!-- Supported languages -->
    <extend-property name="locale" values="nl"/>
    <extend-property name="locale" values="en_US"/>  
</module>